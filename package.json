{
  "name": "robot-backend",
  "version": "0.0.1",
  "description": "Robot project -> Gateway Api",
  "main": "index.js",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "i": "npm i --registry=https://registry.npm.taobao.org",
    "pm2": "pm2",
    "babel-node": "babel-node",
    "start": "better-npm-run start",
    "compile": "npx babel src --out-dir dist",
    "prod": "npm run i && npm run deploy:prod",
    "prod:restart": "npm run i && npm run deploy:restart",
    "deploy": "better-npm-run deploy",
    "deploy:restart": "better-npm-run deploy:restart",
    "deploy:test": "better-npm-run deploy:test",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "test": "NODE_ENV=test mocha --compilers js:babel-register --timeout 15000 tests/**/*.test.js ",
    "test:one": "NODE_ENV=test mocha --compilers js:babel-register --timeout 15000",
    "coverage": "istanbul cover _mocha -- --compilers js:babel-register --timeout 15000 tests/**/*.test.js"
  },
  "betterScripts": {
    "deploy": {
      "command": "pm2 start deploy/app.json",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:restart": {
      "command": "pm2 restart deploy/app.json",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:test": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "nodemon src/app.js --exec 'npx babel-node'",
      "env": {
        "DEBUG": "app:*"
      }
    }
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "dependencies": {
    "@alicloud/sms-sdk": "^1.0.9",
    "ali-oss": "^5.1.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "better-npm-run": "0.0.14",
    "co": "^4.6.0",
    "compressible": "^2.0.14",
    "cron-parser": "^2.4.3",
    "debug": "^2.6.1",
    "get-pixels": "^3.3.0",
    "getui-rest-sdk": "^0.2.1",
    "humanize-ms": "^1.2.1",
    "iconv-lite": "^0.4.15",
    "imageinfo": "^1.0.4",
    "jsonwebtoken": "^7.3.0",
    "koa": "^1.2.4",
    "koa-better-body": "^3.0.4",
    "koa-bunyan-logger": "^1.3.0",
    "koa-middlewares": "^6.0.0",
    "koa-route": "^2.4.2",
    "koa-router": "^5.4.0",
    "mime-types": "^2.1.19",
    "moment": "^2.22.2",
    "mongoose": "^4.8.1",
    "mqtt": "^2.17.0",
    "mysql": "^2.13.0",
    "mz": "^2.7.0",
    "orm": "^3.2.3",
    "pdfkit": "^0.8.3",
    "pm2": "^2.3.0",
    "qr-image": "^3.2.0",
    "redis": "^2.6.5",
    "redis-commands": "^1.3.5",
    "request": "^2.79.0",
    "safe-buffer": "^5.0.1",
    "safe-timers": "^1.1.0",
    "sequelize": "^3.30.2",
    "sequelize-cli": "^4.0.0",
    "socket.io": "^2.1.1",
    "socket.io-redis": "^5.2.0",
    "taobao-sdk": "^2.0.5",
    "uid-safe": "^2.1.3",
    "url": "^0.11.0",
    "utility": "^1.10.0",
    "uuid": "^3.2.1",
    "v8-profiler": "^5.7.0",
    "xml2js": "^0.4.17",
    "yaml": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "co-mocha": "^1.2.0",
    "co-supertest": "0.0.10",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-promise": "^1.0.8",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "nodemon": "^1.17.3",
    "should": "^11.2.0",
    "supertest": "^3.0.0"
  }
}
